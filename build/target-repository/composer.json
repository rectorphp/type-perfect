{
    "name": "rector/type-perfect",
    "type": "phpstan-extension",
    "description": "Next level type declaration checks",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "phpstan/phpstan": "^2.0",
        "webmozart/assert": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "Rector\\TypePerfect\\": "src"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/extension.neon"
            ]
        }
    }
}
